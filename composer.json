{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "barryvdh/laravel-cors": "1.0.5",
        "fzaninotto/faker": "1.9.1",
        "intervention/image": "2.5.1",
        "jenssegers/mongodb-session": "1.3.0",
        "laracasts/flash": "3.1",
        "laravel/framework": "7.1.2",
        "laravel/passport": "8.4.1",
        "laravel/tinker": "2.2.0",
        "laravelcollective/html": "6.1.0",
        "league/flysystem-aws-s3-v3": "1.0.24",
        "maatwebsite/excel": "3.1.19",
        "mockery/mockery": "1.3.1",
        "moloquent/moloquent": "0.4",
        "phpunit/phpunit": "9.0.1"
    },
    "require-dev": {

    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files" : [
          "app/Helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize",
            "php artisan passport:install"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
